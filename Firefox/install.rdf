<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>amarok@enconn.fr</em:id>
    <em:name>zds-notif</em:name>
    <em:description>Une extension pour connaitre le nombre de notifications ou MP de ZesteDeSavoir sans avoir besoin d'ouvrir le site</em:description>
    <em:version>0.0.0</em:version>
    <em:creator>AmarOk</em:creator>
    <em:homepageURL>chrome://zds-notif/content/welcome.html</em:homepageURL>
    <em:iconURL>chrome://zds-notif/skin/images/icone_48.png</em:iconURL>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>40.*</em:maxVersion>
